{
  "name": "overlayed",
  "description": "A discord overlay",
  "version": "1.0.0-alpha8",
  "author": "Hacksore",
  "private": true,
  "dependencies": {
    "@hacksore/discord-rpc": "^5.0.0-beta2",
    "@reduxjs/toolkit": "^1.6.2",
    "@types/react-router-dom": "^5.3.2",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "electron-is-dev": "^2.0.0",
    "electron-store": "^8.0.1",
    "express": "^4.17.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-hook-form": "^7.20.5",
    "react-redux": "^7.2.6",
    "react-router-dom": "^6.0.2"
  },
  "scripts": {
    "clean": "rimraf yarn.lock && rimraf ./**/.next && rimraf dist && rimraf ./**/out && rimraf {**/,}node_modules",
    "dev": "cross-env NODE_ENV=development node -r esbuild-register ./scripts/dev.ts",
    "dev:renderer": "vite",
    "dev:main": "cross-env NODE_ENV=development node -r esbuild-register ./scripts/dev.ts",
    "build": "rimraf dist && vite build && npm run prod:esbuild",
    "prod:esbuild": "cross-env NODE_ENV=production node -r esbuild-register ./scripts/prod.ts",
    "prod:tsc": "tsc --project ./src/main/tsconfig.json",
    "pack": "electron-builder --x64 -p never -c electron-builder.json"
  },
  "homepage": "./",
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "devDependencies": {
    "@craco/craco": "^6.4.2",
    "@emotion/react": "^11.5.0",
    "@emotion/styled": "^11.3.0",
    "@mui/icons-material": "^5.2.0",
    "@mui/material": "^5.2.2",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^12.8.3",
    "@types/react": "^17.0.33",
    "@types/react-dom": "^17.0.10",
    "@types/redux-logger": "^3.0.9",
    "concurrently": "^6.4.0",
    "cross-env": "^7.0.3",
    "electron": "^16.0.1",
    "electron-builder": "^22.14.5",
    "electron-notarize": "^1.1.1",
    "redux-logger": "^3.0.6",
    "typescript": "^4.4.4",
    "wait-on": "^6.0.0",
    "@sync-labs/eslint-config": "^2.2.6",
    "@types/node": "^14.14.31",
    "@vitejs/plugin-react-refresh": "^1.1.0",
    "chalk": "^4.1.0",
    "env-cmd": "^10.1.0",
    "esbuild": "^0.8.53",
    "esbuild-register": "^2.0.0",
    "eslint": "^7.20.0",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "vite": "^2.0.4"
  },
  "browserslist": {
    "production": [
      "last 1 electron version"
    ],
    "development": [
      "last 1 electron version"
    ]
  }
}
