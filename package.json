{
  "name": "overlayed",
  "description": "A discord overlay",
  "version": "1.0.0-alpha5",
  "author": "Hacksore",
  "private": true,
  "dependencies": {
    "@hacksore/discord-rpc": "^5.0.0-alpha1",
    "@reduxjs/toolkit": "^1.6.2",
    "electron-is-dev": "^2.0.0",
    "electron-store": "^8.0.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.6"
  },
  "main": "public/electron/main.js",
  "scripts": {
    "react": "cross-env BROWSER=none react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "start": "concurrently -k npm:react npm:electron",
    "electron": "wait-on http://localhost:3000 && electronmon .",
    "pack": "electron-builder --x64 -p never -c electron-builder.json",
    "pack:macos": "electron-builder --x64 --arm64 -p never -c electron-builder.json"
  },
  "homepage": "./",
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "devDependencies": {
    "@emotion/react": "^11.5.0",
    "@emotion/styled": "^11.3.0",
    "@mui/icons-material": "^5.0.5",
    "@mui/material": "^5.0.6",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^12.8.3",
    "@types/react": "^17.0.33",
    "@types/react-dom": "^17.0.10",
    "@types/redux-logger": "^3.0.9",
    "concurrently": "^6.3.0",
    "cross-env": "^7.0.3",
    "electron": "^16.0.1",
    "electron-builder": "^22.13.1",
    "electron-notarize": "^1.1.1",
    "electronmon": "^2.0.2",
    "react-scripts": "4.0.3",
    "redux-logger": "^3.0.6",
    "typescript": "^4.4.4",
    "wait-on": "^6.0.0"
  },
  "browserslist": {
    "production": [
      "last 1 electron version"
    ],
    "development": [
      "last 1 electron version"
    ]
  }
}
